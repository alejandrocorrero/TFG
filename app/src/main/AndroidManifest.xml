<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.correro.alejandro.tfg">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".patient.MainActivityPatient"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".patient.patientfragment.historialfragment.HistorialDetailActivity" />
        <activity android:name=".patient.citatefragment.CitationActivity" />
        <activity android:name=".patient.consultfragment.ConsultActivity" />
        <activity android:name=".patient.consultfragment.ConsultDetailActivity" />
        <activity
            android:name=".medic.MainMedicActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".medic.diaryfragment.DiaryDetailActivity" />
        <activity android:name=".medic.econsultfragment.EConsultDetailActivity" />
        <activity android:name=".medic.econsultfragment.EConsultAddActivity" />
        <activity android:name=".medic.searchfragment.SearchDetailActivity">
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity android:name=".medic.SearchResultsActivity">
        <intent-filter>
            <action android:name="android.intent.action.SEARCH" />
        </intent-filter>
    </activity>
    </application>

</manifest>